package labb1;

import java.util.Random;
public class PlayWithCharacters {
	public static void main (String []args){
		Random randomno = new Random();

		
		String hej = "sol";
		int b0 = (int) hej.charAt(0) + randomno.nextInt(128); // Lite b√§ttre kryptering #BetterThanSONY
		int b1 = (int) hej.charAt(1) + randomno.nextInt(128);
		int b2 = (int) hej.charAt(2) + randomno.nextInt(128);
		
		String ord2="" + (char)b0+(char)b1+(char)b2;
		
		System.out.println( ord2 );
	}
}
