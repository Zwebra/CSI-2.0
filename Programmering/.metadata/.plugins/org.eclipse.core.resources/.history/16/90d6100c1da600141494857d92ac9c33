package labb1;
import java.util.Random;
public class RollingDice {
	public static void main(String []args){
		Random randomno = new Random();
		
		int tarning1 = randomno.nextInt(6) +1;
		int tarning2 = randomno.nextInt(6) +1;
		
		System.out.println("Tärning ett värde:" + tarning1 + ". Tärning två värde: " +  tarning2 + ". Totalt värde:" + (tarning1 + tarning2) + ".");
	
	}
}
